            <TextBlock Height="35" Text="Denne applikation er blevet oprettet på &#x0a; baggrund af læringsmål:" Margin="10"></TextBlock>
            <ListView Name="Learning" MaxWidth="280" Height="180" Margin="0,0,-0.4,0">
                
            </ListView>
        </StackPanel>
    </Grid>
</Window>


@model IEnumerable<Model.Member>

@{
    ViewBag.Title = "Index";
}

<h2>Oversigt over medlemmer i Spiseklubbenn</h2>
<div class="container">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.ActionLink("Detaljer", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Opsig Medlemskab", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
</div>